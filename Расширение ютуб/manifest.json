{
  "name": "Getting Started Example",
  "version": "1.0",
  "description": "Build an Extension!",
  "permissions": ["activeTab","declarativeContent", "storage","https://*.youtube.com/"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "page_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "ytr16.png",
      "32": "ytr32.png",
      "48": "ytr48.png",
      "128": "ytr128.png"
    }
  },
  "icons": {
    "16": "ytr16.png",
    "32": "ytr32.png",
    "48": "ytr48.png",
    "128": "ytr128.png"
  },
  "manifest_version": 2
}